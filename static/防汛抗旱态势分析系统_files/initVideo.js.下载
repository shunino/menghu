//地图加载视频站点
// $(function () {
//     note.on("subject.station.checked", (event,checked, layer)=>{
//         console.info(layer)
//         console.info(checked)
//     });
// })
function loadVideoLayer(isShow) {
  var videoPointLayer;
  var videoLayerID = "videoLayerID";
  if (isShow) {
    ajaxCallJson({}, '/video/getAllVideoCode', function (_data) {
      if (_data && _data.data) {
        require(["myMap/layer/PointLayer"], function (PointLayer) {
          videoPointLayer = new PointLayer(_data.data.mapPointArr, _data.data.classBreakInfos, videoLayerID);
          map.add(videoPointLayer);
        });
      }
    });
  } else {
    toRemoveMapPoint(videoLayerID)
  }
}

// initVideoData();视频站点 鼠标双击事件调用
function initVideoData(obj) {
  videoUrl = '/video/getVideoView?id=' + obj.mocd;
  videoWindow = new MyWindow();
  videoWindow.isClose = true;
  videoWindow.setWindow(videoUrl, {title: "视频图像", zIndex: 9999});
  videoWindow.show();
}

$(function () {
  note.on("subject.station.checked", (event, checked, station) => {
    if (station.code === "09") {
      // loadVideoLayer(station.checked);
    }
  })
});