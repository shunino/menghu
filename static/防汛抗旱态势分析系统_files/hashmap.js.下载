function MapObj() {
  this.container = new Object();
}


MapObj.prototype.put = function (key, value) {
  this.container[key] = value;
}


MapObj.prototype.get = function (key) {
  return this.container[key];
}


MapObj.prototype.keySet = function () {
  var keyset = new Array();
  var count = 0;
  for (var key in this.container) {
    // 跳过object的extend函数
    if (key == 'extend') {
      continue;
    }
    keyset[count] = key;
    count++;
  }
  return keyset;
}


MapObj.prototype.size = function () {
  var count = 0;
  for (var key in this.container) {
    // 跳过object的extend函数
    if (key == 'extend') {
      continue;
    }
    count++;
  }
  return count;
}


MapObj.prototype.remove = function (key) {
  delete this.container[key];
}


MapObj.prototype.toString = function () {
  var str = "";
  for (var i = 0, keys = this.keySet(), len = keys.length; i < len; i++) {
    str = str + keys[i] + "=" + this.container[keys[i]] + ";\n";
  }
  return str;
}